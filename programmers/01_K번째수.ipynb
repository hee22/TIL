    "answers = []\n",
    "def solution(array, commands):\n",
    "    for command in commands:\n",
    "        i = command[0]\n",
    "        j = command[1]\n",
    "        k = command[2]\n",
    "\n",
    "        a = array[i-1:j]\n",
    "        a.sort()\n",
    "        answer = a[k-1]\n",
    "        answers.append(answer)\n",
    "    return answers"
   ]
  }
